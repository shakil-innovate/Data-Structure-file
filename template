ll add(ll a,ll b){ return ((a%mod + b%mod)+ mod ) % mod;}
ll sub(ll a,ll b){ return ((a%mod - b%mod)+mod)% mod;}
ll mul(ll a,ll b){ return ((a%mod) * (b%mod))%mod ;}
ll power(ll a, ll b){ll res=1; a%=mod;while(b > 0){if(b & 1)res=((i128)res*a) %mod; a=((i128)a*a)%mod;b=b>>1;}return res;}
ll inv(ll a){return power(a,mod-2);}

ll nCr(ll n, ll r) {if (r >n - r) r= n-r; ll res = 1; for (ll i = 0; i < r; ++i) {res =((res * (n - i))%mod *inv (i + 1))%mod;}
   return res;}
